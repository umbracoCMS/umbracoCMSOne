{
    "$schema": "appsettings-schema.json",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        }
    },
    "Umbraco": {
        "CMS": {
            "Global": {
                "Id": "22d6e6c7-cff5-42f2-8846-068ee614c21f",
                "UseHttps": true,
                "SanitizeTinyMce": true
            },
            "Content": {
                "AllowEditInvariantFromNonDefault": true,
                "ContentVersionCleanupPolicy": {
                    "EnableCleanup": true
                }
            },
            "Unattended": {
                "UpgradeUnattended": true
            },
            "Runtime": {
                "Mode": "Production"
            },
            "Security": {
                "AllowConcurrentLogins": false
            },
            "ModelsBuilder": {
                "ModelsMode": "SourceCodeAuto"
            }
        }
    },
    "ConnectionStrings": {
        "umbracoDbDSN": "Server=tcp:umbraco-sql-zerdan.database.windows.net,1433;Initial Catalog=umbraco-cms-db;Persist Security Info=False;User ID=hans@onatrix.com;Password=BytMig123!;MultipleActiveResultSets=True;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
    }
